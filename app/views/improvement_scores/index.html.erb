<h1>Listing improvement_scores</h1>

<table>
  <thead>
    <tr>
      <th>Comms question</th>
      <th>Improvement plan</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @improvement_scores.each do |improvement_score| %>
      <tr>
        <td><%= improvement_score.comms_question %></td>
        <td><%= improvement_score.improvement_plan %></td>
        <td><%= improvement_score.score %></td>
        <td><%= link_to 'Show', improvement_score %></td>
        <td><%= link_to 'Edit', edit_improvement_score_path(improvement_score) %></td>
        <td><%= link_to 'Destroy', improvement_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Improvement score', new_improvement_score_path %>
