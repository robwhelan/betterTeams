<div class="col-md-6">
	<h3><span class="glyphicon glyphicon-user" style="color:rgba(220,220,220,1);"></span><%= @job_posting.title %></h3>
	<ul id="job-discs" class="list-unstyled">
	<% @job_discs.each do |disc| %>
		<li class="btn btn-success btn-lg btn-block job-traits" data-job-id="<%= disc.id %>"><%= disc.name %></li>
	<% end %>
	</ul>
</div>

<div class="col-md-6">
	<h3><span class="glyphicon glyphicon-user" style="color:rgba(151,187,205,1);"></span><%= @user.first_name %></h3>
	<ul id="user-discs" class="list-unstyled">
	<% @user_discs.each do |disc| %>
		<li class="btn btn-default btn-lg btn-block user-traits" data-user-id="<%= disc[1][1] %>"><%= disc[0] %></li>
	<% end %>
	</ul>
</div>

