<div class="col-md-6">
	<h3><span class="glyphicon glyphicon-user" style="color:rgba(220,220,220,1);"></span><%= @job_posting.title %></h3>
	<ul id="job-values" class="list-unstyled">
	<% @job_values.each do |value| %>
		<li class="btn btn-success btn-lg btn-block job-traits" data-job-id="<%= value.id %>"><%= value.name %></li>
	<% end %>
	</ul>
</div>

<div class="col-md-6">
	<h3><span class="glyphicon glyphicon-user" style="color:rgba(151,187,205,1);"></span><%= @user.first_name %></h3>
	<ul id="user-values" class="list-unstyled">
	<% @user_values.each do |value| %>
		<li class="btn btn-default btn-lg btn-block user-traits" data-user-id="<%= value[1][1] %>"><%= value[0] %></li>
	<% end %>
	</ul>
</div>

