<h1>Listing job_benchmarks</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_benchmarks.each do |job_benchmark| %>
      <tr>
        <td><%= job_benchmark.title %></td>
        <td><%= job_benchmark.description %></td>
        <td><%= link_to 'Show', job_benchmark %></td>
        <td><%= link_to 'Edit', edit_job_benchmark_path(job_benchmark) %></td>
        <td><%= link_to 'Destroy', job_benchmark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job benchmark', new_job_benchmark_path %>
