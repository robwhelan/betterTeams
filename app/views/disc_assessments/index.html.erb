<h1>Listing disc_assessments</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Driver score</th>
      <th>Influencer score</th>
      <th>Sociable score</th>
      <th>Conscientious score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @disc_assessments.each do |disc_assessment| %>
      <tr>
        <td><%= disc_assessment.user %></td>
        <td><%= disc_assessment.driver_score %></td>
        <td><%= disc_assessment.influencer_score %></td>
        <td><%= disc_assessment.sociable_score %></td>
        <td><%= disc_assessment.conscientious_score %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>