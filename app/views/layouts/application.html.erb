	  ga('create', 'UA-59095156-1', 'auto');
	  ga('send', 'pageview');

	</script>
</head>
<body>

	<p class="notice"><%= notice %></p>
	<p class="alert"><%= alert %></p>
	<%= yield %>
	<ul class="hidden">
		<%= render 'devise/menu/login_items' %>
	</ul>
	<%= yield :javascript %>
</body>
</html>
